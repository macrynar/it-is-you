{
  "redirects": [
    { "source": "/character-sheet", "destination": "/character", "permanent": true }
  ],
  "rewrites": [
    { "source": "/pricing", "destination": "/pricing.html" },
    { "source": "/methodology", "destination": "/methodology.html" },
    { "source": "/user-profile-tests", "destination": "/user-profile-tests.html" },
    { "source": "/auth/:path*", "destination": "/app.html" },
    { "source": "/test/:path*", "destination": "/app.html" },
    { "source": "/test", "destination": "/app.html" },
    { "source": "/settings", "destination": "/app.html" },
    { "source": "/settings/:path*", "destination": "/app.html" },
    { "source": "/share", "destination": "/app.html" },
    { "source": "/share/:path*", "destination": "/app.html" },
    { "source": "/character", "destination": "/app.html" },
    { "source": "/character/:path*", "destination": "/app.html" },
    { "source": "/demo-card", "destination": "/app.html" },
    { "source": "/demo-card/:path*", "destination": "/app.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/demo-card",
      "headers": [
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" }
      ]
    }
  ]
}
