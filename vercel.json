{
  "rewrites": [
    { "source": "/character-sheet", "destination": "/app.html" },
    { "source": "/user-profile-tests", "destination": "/user-profile-tests.html" },
    { "source": "/auth/:path*", "destination": "/app.html" },
    { "source": "/test/:path*", "destination": "/app.html" },
    { "source": "/test", "destination": "/app.html" },
    { "source": "/settings", "destination": "/app.html" },
    { "source": "/settings/:path*", "destination": "/app.html" },
    { "source": "/character", "destination": "/app.html" },
    { "source": "/character/:path*", "destination": "/app.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
